const mongoose=require("mongoose");
const studentModelSchema=mongoose.Schema({
    status:
    {
        type:String,
        default:"Active"
},
admissionNumber:{
    type:String
},
name:{
    type:String
},
department:{
    type:String
},
semester:{
    type:String
},
phone:{
    type:String
},
dateOfBirth:{
    type:Date
}
})
module.exports=mongoose.model("studentModel",studentModelSchema);